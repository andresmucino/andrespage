{
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
    },
    "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
    //   "plugin:vue/vue3-essential",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "airbnb",
      "airbnb-typescript",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "vue",
      "prettier",
      "react",
      "@typescript-eslint"
    ],
    "rules": {
        // "vue/no-multiple-template-root": "off"
    }
  }